{
  "name": "rapydo",
  "version": "0.7.2",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --poll 1000 --aot",
    "start-cypress": "ng serve --host 0.0.0.0 --port 8080 --aot --disable-host-check",
    "courtesy": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist",
    "build": "ng build --prod --optimization=true --extractCss=false",
    "gzip": "gzipper --verbose /app/dist",
    "test": "ng test --code-coverage",
    "single-test": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "license": "MIT",
  "dependencies": {

    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@angular/animations": "8.2.14",

    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "tslib": "1.10.0",

    "bootstrap": "4.4.1",
    "@ng-bootstrap/ng-bootstrap": "5.2.1",
    "ngx-clipboard": "12.3.0",
    "@ngx-formly/core": "5.5.10",
    "@ngx-formly/bootstrap": "5.5.10",
    "@swimlane/ngx-datatable": "16.0.3",
    "moment": "2.24.0",
    "ngx-moment": "3.5.0",
    "angular-confirmation-popover": "4.2.1",
    "ngx-uploadx": "3.3.3",
    "file-saver": "2.0.2",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.12.0",
    "material-icons": "0.3.1",
    "ngx-toastr": "11.3.2",
    "ngx-google-analytics": "8.0.0",
    "@sentry/browser": "5.11.1",
    "ngx-device-detector": "1.3.20",

    "core-js": "3.6.4",
    "zone.js": "0.10.2",
    "regenerator-runtime": "0.13.3",
    "angular-polyfills": "1.0.1",
    "ie-shim": "0.1.0"
  },
  "devDependencies": {
    "@angular/cli": "8.3.23",
    "typescript": "3.5.3",

    "@angular-devkit/build-angular": "0.803.23",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",

    "@types/node": "13.1.8",
    "@types/jasmine": "3.5.1",
    "@types/jasminewd2": "2.0.8",

    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "protractor": "5.4.2",

    "codelyzer": "5.2.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "raw-loader": "4.0.0",

    "rimraf": "3.0.0",
    "copyfiles": "2.2.0",
    "gzipper": "3.4.2"
  }
