{
  "name": "rapydo",
  "version": "0.7.3",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --poll 1000 --aot",
    "start-cypress": "ng serve --host 0.0.0.0 --port 8080 --aot --disable-host-check",
    "courtesy": "rimraf dist/* && copyfiles --flat /app/courtesy/index.html /app/dist",
    "build": "ng build --prod --optimization=true --buildOptimizer=true --extractCss=false --extractLicenses=true --progress=true",
    "gzip": "gzipper --verbose /app/dist",
    "test": "ng test --code-coverage",
    "single-test": "ng test --code-coverage --watch=false",
    "e2e": "ng e2e"
  },
  "license": "MIT",
  "dependencies": {

    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@angular/animations": "9.1.0",
    "@angular/localize": "9.1.0",

    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",

    "bootstrap": "4.4.1",
    "@ng-bootstrap/ng-bootstrap": "6.0.2",
    "ngx-clipboard": "13.0.0",
    "@ngx-formly/core": "5.5.15",
    "@ngx-formly/bootstrap": "5.5.15",
    "@swimlane/ngx-datatable": "16.0.3",
    "moment": "2.24.0",
    "ngx-moment": "3.5.0",
    "angular-confirmation-popover": "4.2.2",
    "ngx-uploadx": "3.3.4",
    "file-saver": "2.0.2",
    "@types/file-saver": "2.0.1",
    "angular2-cookie-law": "7.0.1",
    "@fortawesome/fontawesome-free": "5.13.0",
    "material-icons": "0.3.1",
    "ngx-toastr": "12.0.0",
    "ngx-google-analytics": "8.0.0",
    "@sentry/browser": "5.15.0",
    "ngx-device-detector": "1.4.1",
    "ngx-spinner": "9.0.1",

    "core-js": "3.6.4",
    "zone.js": "0.10.3",
    "angular-polyfills": "1.0.1",
    "ie-shim": "0.1.0"
  },
  "devDependencies": {
    "typescript": "3.8.3",

    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",

    "@angular-devkit/build-angular": "0.901.0",

    "@types/node": "13.9.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",

    "karma": "4.4.1",
    "jasmine-core": "3.5.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-spec-reporter": "0.0.32",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "jasmine-spec-reporter": "5.0.1",
    "protractor": "5.4.3",

    "raw-loader": "4.0.0",

    "rimraf": "3.0.2",
    "copyfiles": "2.2.0",
    "gzipper": "3.5.0"
  }
}
