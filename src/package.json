{
  "name": "rapydo",
  "version": "2.1.0",
  "private": true,
  "description": "Frontend for RAPyDo framework",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --watch --live-reload",
    "start:for-cypress": "ng serve --configuration cypress --host 0.0.0.0 --port 8080 --disable-host-check",
    "courtesy": "mkdir -p /app/dist && rm -rf /app/dist/* && cp /app/courtesy/index.html /app/dist/",
    "build": "ng build --configuration=production",
    "build:ssr": "ng run RAPyDo:server:production && ng run RAPyDo:prerender",
    "move-build-online": "rm -rf /app/dist_online && mv /app/dist /app/dist_online",
    "gzip": "gzipper compress --verbose --workers 2 --level 9 --threshold 512 --remove-larger --exclude png,jpeg,jpg,svg,ico,woff,woff2,gif,webp /app/dist",
    "test": "ng test --code-coverage",
    "test:single": "ng test --code-coverage --watch=false",
    "cypress": "cypress run",
    "cypress:start:full": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group full --browser chrome'",
    "cypress:start:totp": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group totp --browser chrome'",
    "cypress:start:lite": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group lite --browser chrome'",
    "cypress:start:hiddenlogin": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --record --group hiddenlogin --browser chrome'",
    "cypress:start:norecord": "start-server-and-test start:for-cypress http://localhost:8080 'cypress run --browser chrome'"
  },
  "license": "MIT",
  "dependencies": {
    "typescript": "4.3.5",
    "tslib": "2.3.1",
    "@types/node": "16.9.1",
    "@angular/common": "12.2.6",
    "@angular/compiler": "12.2.6",
    "@angular/core": "12.2.6",
    "@angular/forms": "12.2.6",
    "@angular/platform-browser": "12.2.6",
    "@angular/platform-browser-dynamic": "12.2.6",
    "@angular/router": "12.2.6",
    "@angular/animations": "12.2.6",
    "@angular/localize": "12.2.6",
    "@angular/language-service": "12.2.6",
    "@angular/platform-server": "12.2.6",
    "@angular/compiler-cli": "12.2.6",
    "@angular/cli": "12.2.6",
    "@angular-devkit/build-angular": "12.2.6",
    "@nguniversal/builders": "12.1.0",
    "ngx-build-plus": "12.2.0",
    "core-js": "3.17.3",
    "zone.js": "0.11.4",
    "rxjs": "7.1.0",
    "bootstrap": "4.6.0",
    "@ng-bootstrap/ng-bootstrap": "10.0.0",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@ngx-formly/core": "5.10.22",
    "@ngx-formly/bootstrap": "5.10.22",
    "@ng-select/ng-select": "7.2.0",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@swimlane/ngx-datatable": "19.0.0",
    "moment": "2.29.1",
    "ngx-moment": "5.0.0",
    "ngx-spinner": "12.0.0",
    "ngx-clipboard": "14.0.1",
    "ngx-uploadx": "4.1.3",
    "file-saver-es": "2.0.5",
    "@types/file-saver": "2.0.3",
    "angular2-cookie-law": "7.0.1",
    "ngx-toastr": "14.0.0",
    "ngx-google-analytics": "11.2.1",
    "@sentry/browser": "6.12.0",
    "ngx-device-detector": "2.1.1",
    "xlsx": "0.17.2",
    "ajv": "^8.0.0",
    "ajv-formats": "2.1.1",
    "angular-polyfills": "1.0.1",
    "localstorage-polyfill": "1.0.1",
    "gzipper": "5.0.1"
  },
  "devDependencies": {
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-spec-reporter": "0.0.32",
    "karma-jasmine": "4.0.1",
    "@types/jasmine": "3.9.1",
    "cypress": "8.4.0",
    "@cypress/code-coverage": "3.9.11",
    "cypress-file-upload": "5.0.8",
    "istanbul-instrumenter-loader": "3.0.1",
    "nyc": "15.1.0",
    "start-server-and-test": "1.12.5",
    "otpauth": "7.0.6",
    "node-xlsx": "0.17.1"
  },
  "browserslist": [
    "defaults",
    "cover 99%",
    "not safari < 9",
    "not ios_saf < 9",
    "not ie < 99",
    "not ie_mob < 99",
    "not firefox < 40",
    "not chrome < 40"
  ]
}
